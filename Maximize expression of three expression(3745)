int maximizeExpressionOfThree(int* nums, int numsSize) {
    int i,min=100,max1=-100,max2=-100;
    for(i=0;i<numsSize;i++)
    {
        if(nums[i]<min)
        {
            min=nums[i];
        }
        if(nums[i]>max1&&nums[i]>max2)
        {
            max2=max1;
            max1=nums[i];
        }
        else if(nums[i]>max2)
        {
            max2=nums[i];   
        }
    }
    return max1+max2-min;
}
