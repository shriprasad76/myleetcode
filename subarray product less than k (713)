int numSubarrayProductLessThanK(int* nums, int numsSize, int k) {
    if (k <= 1) return 0;

    int left = 0;
    double prod = 1;
    int count = 0;

    for (int right = 0; right < numsSize; right++) {
        prod *= nums[right];

        while (prod >= k && left <= right) {
            prod /= nums[left];
            left++;
        }

        count += (right - left + 1);
    }

    return count;
}
