bool isThree(int n) {
    int count=0;
    int i,f=0;
    
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            {
                count++;
            }
            if(count>3)
            {
                return false;
                break;
            }
        }
    
    if(count==3)
     {
    return true;
      } 
   return false;
    
    
}
