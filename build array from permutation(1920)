/**
 * Note: The returned array must be malloced, assume caller calls free().
 */
int* buildArray(int* nums, int numsSize, int* returnSize) {
    int i,j;
    int *a=(int*)malloc(sizeof(int)*numsSize);
    for(i=0;i<numsSize;i++)
    {
        a[i]=nums[nums[i]];
    }
    * returnSize=numsSize;
    return a;
    
}
