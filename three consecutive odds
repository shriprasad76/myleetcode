bool threeConsecutiveOdds(int* arr, int arrSize) {
    int i,j,count=1;
    for(i=0;i<arrSize-1;i++)
    {
        if(arr[i]%2==1&&arr[i+1]%2==1)
        {
            count++;
        }
        else
        {
            count=1;
        }
        if(count==3)
        {
           return true;
        }
    }

    
    return false;
    
    
}
